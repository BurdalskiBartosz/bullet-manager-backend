datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  login    String  @unique
  email    String  @unique
  password String
  Token    Token[]
}

model Token {
  id     Int    @id @default(autoincrement())
  value  String @unique
  user   User   @relation(fields: [userId], references: [id])
  userId Int
}
